{
  "name": "kafka-ws",
  "version": "0.2.0",
  "private": true,
  "description": "Websocket service with conditional/partial data requests",
  "main": "index.js",
  "scripts": {
    "kafka-ws": "node index.js",
    "dev": "nodemon index.js",
    "test": "nearleyc app/WhereProcessing/Grammar.ne -o app/WhereProcessing/Parser.js && jest --coverage",
    "test:watch": "nearleyc app/WhereProcessing/Grammar.ne -o app/WhereProcessing/Parser.js && jest --watch --coverage",
    "debugtest": "nearleyc app/WhereProcessing/Grammar.ne -o app/WhereProcessing/Parser.js && node --inspect-brk node_modules/.bin/jest --runInBand --coverage=false",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --fix --ext .js .",
    "parser:compile": "nearleyc app/WhereProcessing/Grammar.ne -o app/WhereProcessing/Parser.js",
    "check-licenses": "apache2-license-checker"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dojot"
  },
  "keywords": [
    "dojot",
    "kafka",
    "websocket"
  ],
  "files": [
    "package-lock.json",
    "app/**/*.js",
    "test/",
    "!test/unit/coverage/"
  ],
  "devDependencies": {
    "@bbc/apache2-license-checker": "^1.1.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.21.1",
    "jest": "^25.5.4",
    "nodemon": "^2.0.4",
    "redis-mock": "^0.49.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@dojot/microservice-sdk": "^0.1.9",
    "compression": "^1.7.4",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^5.3.3",
    "express-request-id": "^1.4.1",
    "express-unless": "^0.5.0",
    "express-ws": "^4.0.0",
    "flat": "^5.0.0",
    "http-errors": "^1.7.3",
    "http-status-codes": "^1.4.0",
    "http-terminator": "^3.0.0",
    "json-mask": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "moo": "^0.5.1",
    "morgan": "^1.10.0",
    "nearley": "^2.19.3",
    "path-to-regexp": "^6.1.0",
    "redis": "^3.0.2",
    "uuid": "^8.1.0",
    "ws": "^7.3.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.js",
      "!index.js",
      "!app/App.js",
      "!app/Errors/*",
      "!app/WhereProcessing/Parser.js"
    ],
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/test/**/**.test.js"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "author": "@eduardogmisiuk",
  "contributors": [
    "@mprevide",
    "@thiagodpf",
    "@jonaphael"
  ],
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ]
}
