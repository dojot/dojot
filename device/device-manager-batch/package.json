{
  "name": "typescript-template",
  "version": "1.0.0",
  "description": "This repository is a template for a Typescript project.",
  "author": {
    "name": "Rodrigo Maia",
    "email": "rodrigoasmaia@gmaiul.com"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "keywords": [
    "Typescript",
    "nodemon",
    "jest",
    "Eslint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rodrigoaasm/typescript-template"
  },
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/index.ts",
    "test": "jest --coverage",
    "unit-test": "jest ./tests/units --coverage",
    "integration-test": "jest ./tests/integration --runInBand",
    "check-licenses": "apache2-license-checker",
    "eslint:lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix"
  },
  "devDependencies": {
    "@bbc/apache2-license-checker": "^1.1.4",
    "@swc/core": "^1.3.7",
    "@swc/jest": "^0.2.23",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/node": "^14.18.26",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "install": "^0.13.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.4",
    "npm": "^9.1.2",
    "prisma": "^4.6.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@dojot/microservice-sdk": "^0.2.1",
    "@prisma/client": "^4.6.1",
    "@types/cors": "^2.8.12",
    "body-parse": "^0.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "http-terminator": "^3.2.0",
    "jest-mock-extended": "^3.0.1",
    "joi": "^17.6.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.camelcase": "^4.3.0",
    "multer": "^1.4.5-lts.1"
  }
}
