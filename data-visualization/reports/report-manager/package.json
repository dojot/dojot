{
  "name": "report-manager",
  "description": "API responsible for creating reports",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "author": "Luan Eduardo da Costa",
  "license": "Apache-2.0",
  "scripts": {
    "test": "jest",
    "test:unit": "jest tests/units",
    "test:unit:ci": "jest tests/units --coverage --no-cache",
    "test:integration": "jest tests/integration --runInBand",
    "test:integration:ci": "jest tests/integration --runInBand --no-cache",
    "dev": "nodemon",
    "start": "node dist/index.js",
    "ts:prune": "npx ts-prune",
    "check:updates": "npx npm-check-updates",
    "check:licenses": "apache2-license-checker",
    "eslint:lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "build": "swc src -d dist --copy-files --delete-dir-on-start --ignore **/*.test.ts,**/*.spec.ts"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "devDependencies": {
    "@bbc/apache2-license-checker": "^1.1.4",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14",
    "@swc/jest": "^0.2.23",
    "@types/bull": "^3.15.9",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/pdfmake": "^0.2.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "bull-repl": "^0.29.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prisma": "^4.7.1",
    "supertest": "^6.3.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@dojot/microservice-sdk": "^0.2.2",
    "@prisma/client": "^4.7.1",
    "bull": "^4.10.1",
    "csv-writer": "^1.6.0",
    "form-data": "^4.0.0",
    "joi": "^17.7.0",
    "jszip": "^3.10.1",
    "lodash.camelcase": "^4.3.0",
    "pdfmake": "^0.2.6"
  }
}
